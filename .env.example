# Maple — Environment Variables
# Copy this file to .env and fill in your values.
# All variables are optional — the app degrades gracefully without them.

# Google OAuth2 Client ID (for Calendar + Gmail integration)
# Get from: https://console.cloud.google.com/apis/credentials
VITE_GOOGLE_CLIENT_ID=

# Google Gemini API Key (for AI categorization, email classification, focus suggestions)
# Get from: https://aistudio.google.com/apikey
VITE_GEMINI_API_KEY=

# Ollama (local AI — replaces Gemini for task categorization, email classification, chat)
# Point to your Ollama server. Use /api/ollama for dev (Vite proxy), or direct URL for production.
VITE_OLLAMA_BASE_URL=http://10.0.0.204:11434
VITE_OLLAMA_MODEL=llama3.1:latest

# Supabase (for cloud sync/replication — app works offline without these)
# Get from: https://supabase.com/dashboard/project/_/settings/api
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# ====================== V2 — MAPLE Life OS ======================

# Anthropic API Key (for Claude AI — used by MCP proxy, NOT browser)
# Get from: https://console.anthropic.com/settings/keys
# Note: Set this in the MCP proxy process env, NOT as VITE_ prefix
ANTHROPIC_API_KEY=

# MCP Proxy URL (local proxy for Claude API + MCP servers)
VITE_MCP_PROXY_URL=http://localhost:3100

# MCP Server URLs (optional — defaults to localhost ports below)
VITE_MCP_GOOGLE_WORKSPACE_URL=http://localhost:8010
VITE_MCP_REAL_ESTATE_URL=http://localhost:8011
VITE_MCP_ZILLOW_URL=http://localhost:8012
VITE_MCP_ALPACA_URL=http://localhost:8013
VITE_MCP_NOTION_URL=http://localhost:8014
VITE_MCP_PDF_READER_URL=http://localhost:8015

# Family calendar IDs (comma-separated Google Calendar IDs)
VITE_FAMILY_CALENDAR_IDS=

# Hubstaff Integration (for staffing hours sync)
# Get PAT from: https://account.hubstaff.com/personal_access_tokens
# Org ID from: Hubstaff dashboard URL (e.g., app.hubstaff.com/organizations/515899)
VITE_HUBSTAFF_PAT=
VITE_HUBSTAFF_ORG_ID=

# Plaid Integration (for financial dashboard bank sync)
# Environment: 'sandbox' for testing, 'development' or 'production' for real data
VITE_PLAID_ENV=sandbox

# GitHub Dev Intelligence Dashboard
VITE_GITHUB_PAT=           # Personal Access Token with 'repo' scope for project tracking
